{
  "name": "leapfrog-music",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/",
    "start": "nodemon --exec babel-node src/index.js",
    "build": "npm run clean && npm run babel && cp:views",
    "cp:views": "cp -r src/views dist/views",
    "babel": "babel src/ -d dist/",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback"
  },
  "dependencies": {
    "atlassian-connect-express": "^2.0.0",
    "atlassian-connect-express-hipchat": "^0.3.5",
    "atlassian-connect-express-redis": "^0.1.6",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.4",
    "camelize": "^1.0.0",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-hbs": "^1.0.1",
    "knex": "^0.14.0",
    "lodash": "^4.13.1",
    "morgan": "^1.7.0",
    "nodemon": "^1.12.1",
    "request": "^2.72.0",
    "rsvp": "^3.2.1",
    "snakeize": "^0.1.0",
    "socket.io": "^2.0.4",
    "sqlite3": "^3.1.13",
    "static-expiry": "^0.0.11",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "rimraf": "^2.6.2"
  }
}
