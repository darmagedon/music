{
  "name": "my-add-on",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && npm run babel && npm run cp:views",
    "cp:views": "cp -r src/views dist/views",
    "babel": "babel src/ -d dist/",
    "start": "nodemon --exec babel-node src/app.js",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback"
  },
  "dependencies": {
    "atlassian-connect-express": "^2.0.0",
    "atlassian-connect-express-hipchat": "^0.3.5",
    "atlassian-connect-express-redis": "^0.1.6",
    "axios": "^0.18.0",
    "body-parser": "^1.15.2",
    "babel-polyfill": "^6.26.0",
    "bookshelf": "^0.13.0",
    "camelize": "^1.0.0",
    "compression": "^1.6.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-hbs": "^1.0.1",
    "knex": "^0.14.4",
    "lodash": "^4.13.1",
    "morgan": "^1.7.0",
    "query-string": "^6.0.0",
    "request": "^2.72.0",
    "rsvp": "^3.2.1",
    "snakeize": "^0.1.0",
    "socket.io": "^2.0.4",
    "sqlite3": "^4.0.0",
    "static-expiry": "^0.0.11",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "rimraf": "^2.6.2",
    "nodemon": "^1.17.3"
  }
}
